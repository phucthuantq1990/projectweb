.jumbotron {
    padding-top:9px;
    height:90px;
    width:1140px
}

#accountsfield {
    margin-top:70px;
    width:200px;
    float:left
}

#listaccounts {
    margin-top:70px;
    margin-left:40px;
    width:900px;
    float:left
}

#logoutbtn :HOVER {
    cursor:pointer
}

#tableaccountfield {
    width:100%
}

#tableaccounts thead td {
    font-weight:700
}

#tableaccounts tr :HOVER {
    cursor:pointer
}

.modal-dialog {
    overflow-y:auto
}

.validate {
    color:red
}

.glyphicon-refresh-animate {
    -webkit-animation-name:rotateThis;
    -webkit-animation-duration:2s;
    -webkit-animation-iteration-count:infinite;
    -webkit-animation-timing-function:linear
}

@-webkit-keyframes rotateThis {
from {
    -webkit-transform:rotate(0deg)
}

to {
    -webkit-transform:rotate(360deg)
}
}